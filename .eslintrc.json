{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "import"],
  "rules": {
    "import/no-duplicates": "error",
    "no-unused-vars": "warn",
    "no-redeclare": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
